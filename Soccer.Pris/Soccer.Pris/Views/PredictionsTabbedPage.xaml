<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:prism="http://prismlibrary.com"
            prism:ViewModelLocator.AutowireViewModel="True"
            xmlns:local="clr-namespace:Soccer.Pris.Views"
            x:Class="Soccer.Pris.Views.PredictionsTabbedPage"
            BackgroundColor="{StaticResource ColorBackgroud}"
            Title="{Binding Title}">
    <TabbedPage.Children>
        <local:PredictionsForTournamentPage />
        <local:ClosedPredictionsForTournamentPage />
        <local:PositionsInTournamentPage />
    </TabbedPage.Children>
</TabbedPage>